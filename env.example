# RedFuzz v5.0.0 - Environment Variables Example
# Copy this file to .env and fill in your actual values
# Never commit .env file to version control!

# =============================================================================
# SLACK NOTIFICATION SETTINGS
# =============================================================================
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/WEBHOOK/URL

# =============================================================================
# EMAIL NOTIFICATION SETTINGS
# =============================================================================
EMAIL_USERNAME=your-email@gmail.com
EMAIL_PASSWORD=your-app-password
EMAIL_RECIPIENTS=admin@company.com,security@company.com

# =============================================================================
# PROXY SETTINGS
# =============================================================================
PROXY_URL=http://127.0.0.1:8080
PROXY_USERNAME=proxy_user
PROXY_PASSWORD=proxy_pass

# =============================================================================
# SESSION MANAGEMENT
# =============================================================================
SESSION_TOKEN=your-session-token-here
SESSION_USER=admin
CSRF_TOKEN=your-csrf-token-here

# =============================================================================
# API KEYS (if needed)
# =============================================================================
API_KEY=your-api-key-here
SECRET_KEY=your-secret-key-here

# =============================================================================
# DATABASE CONNECTION (if using database)
# =============================================================================
DB_HOST=localhost
DB_PORT=5432
DB_NAME=redfuzz
DB_USER=redfuzz_user
DB_PASSWORD=your-db-password

# =============================================================================
# MONITORING WEBHOOKS
# =============================================================================
MONITORING_WEBHOOK_URL=https://your-monitoring-service.com/webhook
ALERT_WEBHOOK_URL=https://your-alert-service.com/webhook

# =============================================================================
# SECURITY SETTINGS
# =============================================================================
ENCRYPTION_KEY=your-32-character-encryption-key
SIGNING_KEY=your-signing-key-for-requests

# =============================================================================
# CUSTOM SETTINGS
# =============================================================================
CUSTOM_API_URL=https://your-custom-api.com
CUSTOM_API_TOKEN=your-custom-api-token 